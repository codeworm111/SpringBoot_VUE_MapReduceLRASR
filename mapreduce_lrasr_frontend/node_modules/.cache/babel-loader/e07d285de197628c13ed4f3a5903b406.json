mn>\n                            <el-table-column\n                                    label=\"状态\">\n                                <template slot-scope=\"scope\">\n                                    <span v-if=\"scope.row.status === 1\">准备上传</span>\n                                    <span v-if=\"scope.row.status === 4\" style=\"color: brown\">上传失败</span>\n                                    <span v-if=\"scope.row.status === 5\" style=\"color: chartreuse\">已上传</span>\n                                    <el-progress v-if=\"scope.row.status === 2\" :text-inside=\"true\" :stroke-width=\"20\" :percentage=\"scope.row.percent\"></el-progress>\n                                </template>\n                            </el-table-column>\n                            <el-table-column\n                                    label=\"操作\">\n                                <template slot-scope=\"scope\">\n                                    <el-button type=\"danger\" @click=\"deleteFile(scope.row.id)\">删除</el-button>\n                                </template>\n                            </el-table-column>\n                        </el-table>\n                        <br/>\n                        <el-button type=\"danger\" @click=\"up.start()\" style=\"margin-top: 10px; float: right\">开始上传</el-button>\n                    </div>\n                </div>\n            </el-main>\n\n        </el-container>\n    </el-container>\n  </div>\n\n</template>\n\n<script>\n  export default {\n    name: 'app',\n    data () {\n      return {\n        defaultActive: this.$route.name\n      }\n    },\n    methods: {\n      redirectUrl(val){\n        this.$router.push(val);\n      }\n    }\n  }\n</script>\n\n<style>\n  .el-header {\n      background-color: #3a8ee6;\n      color: #303133;\n      line-height: 60px;\n  }\n\n  .el-aside {\n      color: #333;\n  }\n\n  .my-main {\n    margin: 1em 1em;\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}